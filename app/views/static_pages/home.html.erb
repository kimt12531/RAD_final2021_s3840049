<h1>StaticPages#home</h1>
<div class="center jumbotron">

    <% @questions[0, 5].each do |q| %>
        <p><%= q.title %></p>
        <% q.answers.each do |ans|%>
            <% unless ans[1].nil? %>
                <div><input type="radio" name="question-1-answers"><%= ans[1] %></input></div>
            <% end %>
        <% end %>
    <% end %>
</div>
